{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "shell-app-angular": {
      "projectType": "application", "root": "", "sourceRoot": "src", "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/shell-app-angular", "index": "src/index.html",
            "main": "src/main.ts", "polyfills": ["zone.js"], "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"], "styles": ["src/styles.css"], "scripts": []
          },
          "configurations": { "production": { "outputHashing": "all" } }
        }
      }
    }
  }
}